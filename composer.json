{
    "name": "notifier/meeting",
    "description": "notifier for meeting app",
    "type": "project",
    "minimum-stability": "alpha",
    "require": {
        "symfony/dotenv": "^5.0",
        "monolog/monolog": "^2.0",
        "symfony/mailer": "^5.0",
        "symfony/google-mailer": "^5.0",
        "symfony/routing": "^5.0",
        "symfony/mime": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Settings\\": "src/Settings",
            "Notifier\\": "src/",
            "Mail\\" : "src/Mail",
            "Logs\\" : "src/Logs"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
